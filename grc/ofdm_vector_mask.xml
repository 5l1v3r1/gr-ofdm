<block>
  <name>Vector mask</name>
  <key>ofdm_vector_mask</key>
  <category>FBMC</category>
  <import>import ofdm</import>
  <make>ofdm.vector_mask($vlen, $cut_left, $mask_len, $mask)</make>
  <param>
    <name>Vlen</name>
    <key>vlen</key>
    <value>M</value>
    <type>raw</type>
  </param>
  <param>
    <name>Cut_left</name>
    <key>cut_left</key>
    <value>(M-carriers)/2</value>
    <type>raw</type>
  </param>
  <param>
    <name>Mask_len</name>
    <key>mask_len</key>
    <value>carriers</value>
    <type>raw</type>
  </param>
  <param>
    <name>Mask</name>
    <key>mask</key>
    <value>[]</value>
    <type>raw</type>
  </param>
  <sink>
    <name>in</name>
    <type>complex</type>
    <vlen>$vlen</vlen>
  </sink>
  <source>
    <name>out</name>
    <type>complex</type>
    <vlen>$mask_len</vlen>
  </source>
</block>
